package <%= packageName %>.service.custom;

<%_
const customRepositoryClassName = entityClass + 'CustomRepository';
const customRepositoryInstanceName = entityInstance + 'CustomRepository';

const mapperClassName = entityClass + 'Mapper';
const mapperInstanceName = entityInstance + 'Mapper';

const serviceClassName = entityClass + 'Service';
const customServiceClassName = entityClass + 'CustomService';
_%>
import <%= packageName %>.domain.<%= asEntity(entityClass) %>;
import <%= packageName %>.repository.custom.<%= entityClass %>CustomRepository;
import <%= packageName %>.service.dto.<%= asDto(entityClass) %>;
import <%= packageName %>.service.mapper.<%= entityClass %>Mapper;
import <%= packageName %>.service.<%= serviceClassName %>;

import org.slf4j.Logger;
import org.slf4j.LoggerFactory;

import org.springframework.stereotype.Service;
import org.springframework.transaction.annotation.Transactional;

/**
* Custom Service Implementation for managing {@link <%= asEntity(entityClass) %>}.
*/
@Service
@Transactional
public class <%= customServiceClassName %> extends <%= serviceClassName %> {

    private final Logger log = LoggerFactory.getLogger(<%= customServiceClassName %>.class);

    private final <%= customRepositoryClassName %> <%= customRepositoryInstanceName %>;

    private final <%= mapperClassName %> <%= mapperInstanceName %>;

    public <%= customServiceClassName %>(
        <%= customRepositoryClassName %> <%= customRepositoryInstanceName %>,
        <%= mapperClassName %> <%= mapperInstanceName %>
    ) {
        super(<%= customRepositoryInstanceName %>, <%= mapperInstanceName %>);
        this.<%= customRepositoryInstanceName %> = <%= customRepositoryInstanceName %>;
        this.<%= mapperInstanceName %> = <%= mapperInstanceName %>;
    }
}
